    "@nuxtjs/axios": "^5.10.1",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@tryghost/content-api": "^1.3.8",
    "element-ui": "^2.4.11",
    "express": "^4.16.4",
    "kalendar-vue": "^2.1.0",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.6.1",
    "nuxt-leaflet": "0.0.21",
    "portal-vue": "^2.1.7",
    "vue-gun": "0.0.3",
    "vue-iota-payment": "0.0.31"
